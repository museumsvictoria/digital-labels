<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="DatabaseUrl" value="http://zen.mv.vic.gov.au:8080/" />
    <add key="DatabaseName" value="digitallabels_dev" />
    <add key="EmuServerHost" value="chopin.mv.vic.gov.au"/>
    <add key="EmuServerPort" value="40022"/>
    <add key="MediaPath" value="\\orka\c$\inetpub\digitallabels_stage\media" />
    <add key="MediaServerUrl" value="http://orka.mv.vic.gov.au:8033/" />
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="DescribeMe.Web"/>
    </extensions>
    <targets>
      <target name="file" xsi:type="File" layout="${longdate} [${level}] - ${message}" fileName="${basedir}\logs\log.txt" archiveFileName="${basedir}\logs\log.{#}.txt" archiveAboveSize="1048576" archiveNumbering="Sequence" maxArchiveFiles="9"/>
      <target name="console" xsi:type="ColoredConsole" layout="${longdate} [${level}] - ${message}" useDefaultRowHighlightingRules="true" />
    </targets>
    <rules>
      <logger name="*" minlevel="Error" writeTo="file"/>
      <logger name="DigitalLabels.*" minlevel="Debug" writeTo="file,console"/>
      <logger name="Raven.*" minlevel="Error" writeTo="file"/>
    </rules>
  </nlog>
</configuration>
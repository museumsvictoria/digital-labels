<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DatabaseUrl" value="http://localhost:8081/" />
    <add key="DatabaseName" value="digitallabels_dev" />
    <add key="EmuServerHost" value="localhost" />
    <add key="EmuServerPort" value="40022" />
    <add key="MediaPath" value="D:\Projects\digital-labels\src\DigitalLabels.WebApi\media" />
    <add key="MediaServerUrl" value="http://localhost:59115/" />
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="DescribeMe.Web" />
    </extensions>
    <targets>
      <target name="file" xsi:type="File" layout="${longdate} [${level}] - ${message}" fileName="${basedir}\logs\log.txt" archiveFileName="${basedir}\logs\log.{#}.txt" archiveAboveSize="1048576" archiveNumbering="Sequence" maxArchiveFiles="9" />
      <target name="console" xsi:type="ColoredConsole" layout="${longdate} [${level}] - ${message}" useDefaultRowHighlightingRules="true" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="file" />
      <logger name="DigitalLabels.*" minlevel="Debug" writeTo="file,console" />
      <logger name="Raven.*" minlevel="Error" writeTo="file" />
    </rules>
  </nlog>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>